@model Category_PortoflioVm
@{
    Layout = null;
}

<div class="row">
    <div class="col-md-7">
        <form asp-action="Update" asp-controller="Dashboard_CategoryProtoflio" asp-area="Dashboards" onsubmit="return jQueryAjaxPost(this);">
            @*<div asp-validation-summary="ModelOnly" class="text-danger"></div>*@
            <div class="form-group">
                <label asp-for="Id" class="control-label">@SharedLocalizer["ID:"]</label>
                <input asp-for="Id" type="text" readonly class="form-control" />
                @*<span asp-validation-for="AccountNumber" class="text-danger"></span>*@
            </div>
            <div class="form-group">
                <label asp-for="Category_Name" class="control-label">@SharedLocalizer["NameCategory:"]</label>
                <input asp-for="Category_Name" type="text" class="form-control" />
                <span asp-validation-for="Category_Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value=@SharedLocalizer["Update"] class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
