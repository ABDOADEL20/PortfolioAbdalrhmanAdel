@model IdentityRole

@{
    Layout = null;
}

<div class="row">
    <div class="col-md-9">
        <form asp-action="Delete" asp-controller="Roles" asp-area="Dashboards" method="post">
            <div class="form-group">
                <label asp-for="Id" class="control-label">@SharedLocalizer["ID:"]</label>
                <input asp-for="Id" type="text" readonly class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Name" class="control-label">@SharedLocalizer["Name"]:</label>
                <input asp-for="Name" type="text" readonly class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value=@SharedLocalizer["Delete"] class="btn btn-danger" />
            </div>
        </form>
    </div>
</div>
